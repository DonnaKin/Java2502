package booleanmethods;

public class BooleanTest {

	public static void main(String[] args) {
		boolean b1 = true;
		boolean b2 = false;
		boolean b3 = true;
		boolean b4 = false;
		System.out.println(Boolean.logicalAnd(b1,b2));
		System.out.println(Boolean.logicalAnd(b1,b3));
		System.out.println(Boolean.logicalAnd(b2,b3));
		System.out.println(Boolean.logicalAnd(b2,b4));
	}

}
