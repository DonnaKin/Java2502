package string;

class TranslateKorean{
	public static void Translate(int number){
		int korean;
		switch (korean){
		case 1: "ÀÏ"; break;
		case 2: "ÀÌ"; break;
		case 3: "»ï"; break;
		case 4: "»ç"; break;
		case 5: "¿À"; break;
		case 6: "À°"; break;
		case 7: "Ä¥"; break;
		case 8: "ÆÈ"; break;
		case 9: "±¸"; break;
		}
	}
}
public class NumToKorean {

	public static void main(String[] args) {
		TranslateKorean.Translate(1234567);

	}

}
